{
  "name": "json-xt",
  "version": "0.0.1",
  "description": "The ultimate JSON viewer",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nonsocode/json-schema-viewer.git"
  },
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "build:watch": "__DEV__=true vite build --watch",
    "build:hmr": "rollup --config utils/reload/rollup.config.ts",
    "wss": "node utils/reload/initReloadServer.js",
    "dev": "npm run build:hmr && concurrently -k \"npm: wss\" \"npm: build:watch\"",
    "test": "jest",
    "prepare": "husky install"
  },
  "type": "module",
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "classnames": "^2.3.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
      "concurrently": "^9.2.1",
    "@rollup/plugin-typescript": "^8.5.0",
    "@testing-library/react": "13.4.0",
    "@types/chrome": "0.0.224",
    "@types/jest": "29.0.3",
    "@types/json-to-ast": "^2.1.2",
    "@types/node": "^20.19.0",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.11",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^8.56.1",
    "@typescript-eslint/parser": "^8.56.1",
    "@vitejs/plugin-react": "^5.1.4",
    "chokidar": "^3.5.3",
    "eslint": "^10.0.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "^7.37.5",
    "fs-extra": "11.1.0",
    "husky": ">=6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "29.5.0",
    "lint-staged": ">=10",
    "prettier": "2.7.1",
    "rollup": "^2.80.0",
    "sass": "^1.93.2",
    "ts-jest": "^29.4.4",
    "ts-loader": "9.4.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "ws": "^8.18.1"
  },
  "overrides": {
    "minimatch": "^10.2.1"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  }
}
